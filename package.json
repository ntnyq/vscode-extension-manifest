{
  "name": "vscode-extension-manifest",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@10.2.0",
  "description": "VSCode extension manifest type definitions, validators, and utilities",
  "keywords": [
    "contributes point",
    "extension-manifest",
    "types",
    "validate",
    "vscode",
    "vscode-extension",
    "vscode-types"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vscode-extension-manifest#readme",
  "repository": "ntnyq/vscode-extension-manifest",
  "bugs": {
    "url": "https://github.com/ntnyq/vscode-extension-manifest/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.0-beta.1",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "bumpp": "^10.0.2",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": [
      "eslint --fix",
      "tsx scripts/check.ts"
    ]
  }
}
