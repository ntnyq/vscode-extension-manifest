{
  "name": "vscode-extension-manifest",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@10.10.0",
  "description": "VSCode extension manifest type definitions, validators, and utilities",
  "keywords": [
    "contributes point",
    "extension-manifest",
    "types",
    "validate",
    "vscode",
    "vscode-extension",
    "vscode-types"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vscode-extension-manifest#readme",
  "repository": "ntnyq/vscode-extension-manifest",
  "bugs": {
    "url": "https://github.com/ntnyq/vscode-extension-manifest/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --watch",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest --typecheck",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.1.0",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "^3.1.2",
    "bumpp": "^10.1.0",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,yml,yaml,toml,json}": "eslint --fix"
  }
}
