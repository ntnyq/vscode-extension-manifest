{
  "name": "vscode-extension-manifest",
  "type": "module",
  "version": "0.5.0",
  "packageManager": "pnpm@10.26.2",
  "description": "VSCode extension manifest type definitions, validators, and utilities",
  "keywords": [
    "contributes point",
    "extension-manifest",
    "types",
    "validate",
    "vscode",
    "vscode-extension",
    "vscode-types"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vscode-extension-manifest#readme",
  "repository": "ntnyq/vscode-extension-manifest",
  "bugs": {
    "url": "https://github.com/ntnyq/vscode-extension-manifest/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.8.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.10.4",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.4",
    "tsdown": "^0.18.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,yml,yaml,toml,json}": "eslint --fix"
  }
}
