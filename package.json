{
  "name": "vscode-extension-manifest",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@10.6.2",
  "description": "VSCode extension manifest type definitions, validators, and utilities",
  "keywords": [
    "contributes point",
    "extension-manifest",
    "types",
    "validate",
    "vscode",
    "vscode-extension",
    "vscode-types"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vscode-extension-manifest#readme",
  "repository": "ntnyq/vscode-extension-manifest",
  "bugs": {
    "url": "https://github.com/ntnyq/vscode-extension-manifest/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest --typecheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "quansync": "^0.2.8"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.0",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.8",
    "bumpp": "^10.0.3",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unplugin-quansync": "^0.3.4",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,yml,yaml,json}": "eslint --fix"
  }
}
